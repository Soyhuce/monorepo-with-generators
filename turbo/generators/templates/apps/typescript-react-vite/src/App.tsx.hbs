import { useState } from 'react'

export const App = () => {
  const [count, setCount] = useState(0)

  return (
    <>
      <h1>Vite + React</h1>
      <button
        type="button"
        onClick={() => setCount((count) => count + 1)}
      >
        count is {count}
      </button>
    </>
  )
}
